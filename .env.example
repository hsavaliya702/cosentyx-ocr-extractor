# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# S3 Configuration
S3_BUCKET_NAME=cosentyx-forms
S3_PROCESSED_PREFIX=processed/
S3_FAILED_PREFIX=failed/

# Textract Configuration
TEXTRACT_CONFIDENCE_THRESHOLD=0.85
TEXTRACT_MAX_RETRIES=3

# Bedrock Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20241022-v2:0
BEDROCK_MAX_TOKENS=4096
BEDROCK_TEMPERATURE=0.1

# Application Configuration
LOG_LEVEL=INFO
ENABLE_DUPLICATE_CHECK=true
ENABLE_BEDROCK_VALIDATION=true

# DynamoDB Configuration (optional)
DYNAMODB_TABLE_NAME=cosentyx-extractions
DYNAMODB_REGION=us-east-1
